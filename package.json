{
  "name": "videoextract-cli",
  "version": "1.1.0",
  "description": "A CLI tool to extract videos from JSON timeline files using Remotion",
  "main": "dist/src/index.js",
  "bin": {
    "videoextract": "dist/src/cli.js"
  },
  "scripts": {
    "start": "ts-node src/index.tsx",
    "build": "tsc && chmod +x dist/src/cli.js",
    "render": "ts-node src/index.tsx",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/postinstall.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "video",
    "remotion",
    "cli",
    "extract",
    "timeline",
    "video-processing",
    "video-generation"
  ],
  "author": "Alok Kaushik <alokkaushik5953@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/alok5953/videoextract.git"
  },
  "bugs": {
    "url": "https://github.com/alok5953/videoextract/issues"
  },
  "homepage": "https://github.com/alok5953/videoextract#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "lib/**/*",
    "scripts/**/*",
    "package.json",
    "README.md"
  ],
  "dependencies": {
